ver=$1
test "$ver" = "" && exit 1
cd $HOME/mintstd/binary7-package/usr/bin || exit 1
ln -sf m68k-atari-mint-g++-$ver m68k-atari-mint-g++
ln -sf m68k-atari-mint-gcc-$ver m68k-atari-mint-gcc
ln -sf m68k-atari-mintelf-g++-$ver m68k-atari-mintelf-g++
ln -sf m68k-atari-mintelf-gcc-$ver m68k-atari-mintelf-gcc

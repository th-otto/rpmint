! EURO and CENT
! "Alt" is replaced by "AltGr" else you will not have euro sign!
!
keycode  26 = e  E  currency
keycode  54 = c  C  cent     
keycode 113 = Mode_switch Mode_switch Multi_key
!
! Extra 105 windows keys
!
keycode 115 = Meta_L
keycode 116 = Meta_R
keycode 117 = Menu
!
! Delete & BackSpace support
!
keycode 107 = Delete
keycode 22 = BackSpace
!
! Home & End support
!
keycode 97  = Home
keycode 103 = End
